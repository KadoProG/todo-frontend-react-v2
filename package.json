{
  "name": "todo-frontend-react-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ui:coverage": "vitest --ui --coverage",
    "prepare": "husky",
    "schema": "npm run get:api && npm run schema:generate:json",
    "schema:kotlin": "npm run get:api:kotlin && npm run schema:generate:yml",
    "get:api": "curl -o .docs/api.json http://localhost:8080/docs/api.json && prettier --write .docs/api.json",
    "get:api:kotlin": "curl -o .docs/api.yml http://localhost:8080/api/api-docs.yaml && prettier --write .docs/api.yml",
    "schema:generate:json": "openapi-typescript .docs/api.json -o src/lib/apiClient/types/schema.d.ts && prettier --write src/lib/apiClient/types/schema.d.ts",
    "schema:generate:yml": "openapi-typescript .docs/api.yml -o src/lib/apiClient/types/schema.d.ts && prettier --write src/lib/apiClient/types/schema.d.ts",
    "mock": "prism mock .docs/api.json --port 18080"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "openapi-fetch": "^0.13.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-router-dom": "^7.11.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@stoplight/prism-cli": "^5.12.0",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.80.6",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "25.2.1"
  }
}
